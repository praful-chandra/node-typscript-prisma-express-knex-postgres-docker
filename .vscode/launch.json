{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Node (Docker)",
        "protocol": "inspector",
        "port": 9229,
        "restart": true,
        "localRoot": "${workspaceFolder}/src",
        "remoteRoot": "/app/src",
        "sourceMaps": true,
        "sourceMapPathOverrides": {
          "/app/src/*": "${workspaceFolder}/src/*"
        },
        "skipFiles": [
          "/app/node_modules/**/*.js",
          "<node_internals>/**"
        ]
      }
    ]
  }